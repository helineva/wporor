<p id="notice"><%= notice %></p>

<h1>Beerclubs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Founded</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @beerclubs.each do |beerclub| %>
      <tr>
        <td><%= link_to beerclub.name, beerclub %></td>
        <td><%= beerclub.founded %></td>
        <td><%= beerclub.city %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Beerclub', new_beerclub_path if current_user %>
